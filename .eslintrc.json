{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "strict":[
            "error",
            "global"
        ],
        "quotes":[
            "error",
            "single"
        ]

    }
}
